<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="eRestoran.Client.Mobile.Navigation.MyPage"
             xmlns:pages="clr-namespace:eRestoran.Client.Mobile.Views"
            xmlns:nav="clr-namespace:eRestoran.Client.Mobile.Navigation"
            MasterBehavior="Popover" >
    <MasterDetailPage.Master>
        <nav:MyPageMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:Login />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>